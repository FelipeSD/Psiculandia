<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane prefHeight="576.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.GerenciarInsumo">
   <children>
      <Text layoutX="14.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Gerenciar estoque">
         <font>
            <Font name="Corbel Light" size="17.0" />
         </font>
      </Text>
      <VBox alignment="TOP_RIGHT" layoutX="14.0" layoutY="60.0" prefHeight="200.0" spacing="10.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0">
         <children>
            <HBox alignment="TOP_RIGHT" prefHeight="25.0" prefWidth="411.0" spacing="10.0">
               <children>
                  <Button fx:id="btnAdicionar" mnemonicParsing="false" onAction="#adicionar" text="Adicionar" />
                  <Button fx:id="btnEditar" mnemonicParsing="false" onAction="#editar" text="Editar" />
