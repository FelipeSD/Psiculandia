            <TextField fx:id="txtNome" prefHeight="25.0" prefWidth="257.0" promptText="Nome" />
            <TextField fx:id="txtTipo" prefHeight="25.0" prefWidth="195.0" promptText="Tipo" />
            <TextField fx:id="txtQuantidade" prefHeight="25.0" prefWidth="173.0" promptText="Quantidade inicial (g)" />
            <TextField fx:id="txtValor" prefHeight="25.0" prefWidth="256.0" promptText="Valor" />
            <ComboBox fx:id="cbFornecedor" prefHeight="25.0" prefWidth="195.0" promptText="Fornecedor" />
            <HBox alignment="TOP_RIGHT" prefHeight="25.0" prefWidth="638.0" spacing="10.0">
               <children>
                  <Button fx:id="btnSalvar" mnemonicParsing="false" onAction="#salvar" text="Salvar" />
                  <Button fx:id="btnCancelar" mnemonicParsing="false" onAction="#cancelar" text="Cancelar" />
               </children>
            </HBox>
         </children>
      </FlowPane>
