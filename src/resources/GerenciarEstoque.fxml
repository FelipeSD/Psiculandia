               </children>
            </HBox>
            <TableView fx:id="tableViewInsumo">
              <columns>
                <TableColumn fx:id="cNome" prefWidth="140.6666568517685" text="Nome" />
                <TableColumn fx:id="cFornecedor" prefWidth="155.33331298828125" text="Fornecedor" />
                 <TableColumn fx:id="cValor" prefWidth="155.33331298828125" text="Valor" />
                  <TableColumn fx:id="cQuantidade" prefWidth="155.33331298828125" text="Quantidade" />
              </columns>
